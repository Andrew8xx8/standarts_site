<h2>Добавление информации о компании</h2>
<p><text style="color: red;">Пожалуйста, завершите регистрацию.</text> Шаг 3/3</p>
<%= simple_form_for @company, url: {action: :create}, :html => { :multipart => true } do |f| %>
    <%= render 'layouts/error_message', :obj => f.object %>
    <%= f.input :opf %>
    <%= f.input :name %>
    <%= f.input :logo %>
    <%= f.input :logo_cache, as: :hidden %>
    <%= f.submit class: "btn" %>
<% end %>
